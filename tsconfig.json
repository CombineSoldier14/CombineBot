{
    "compilerOptions": {
      "module": "CommonJS",
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "target": "ES6",
      "outDir": "output",
      "strict": false

    },
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@discordjs/typings",
      "./node_modules/discord-api-types/"
    ],
    "include": ["./*", 
                "./config.json",
                "./core/*",
                "./events/*",
                "./commands/apis/*",
                "./commands/calculator/*",
                "./commands/fun/*",
                "./commands/info/*",
                "./commands/mbti/*",
                "./commands/utility/*",
                "./commands/moderation/*"
              ]
  }
